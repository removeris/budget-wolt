<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="596.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.budgetwolt.fxControllers.ChatController">
   <children>
      <ListView fx:id="messageListView" layoutX="86.0" layoutY="50.0" prefHeight="226.0" prefWidth="430.0" />
      <Button fx:id="sendButton" layoutX="279.0" layoutY="361.0" mnemonicParsing="false" onAction="#sendMessage" text="Send" />
      <TextArea fx:id="messageField" layoutX="86.0" layoutY="286.0" prefHeight="52.0" prefWidth="430.0" promptText="Enter a message" />
      <Button layoutX="86.0" layoutY="361.0" mnemonicParsing="false" onAction="#loadChat" text="Reload" />
   </children>
</AnchorPane>
